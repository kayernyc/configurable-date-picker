{
  "baseUrl": "./",
  "compileOnSave": true,
  "compilerOptions": {
    "downlevelIteration": true,
    "outDir": "dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "target": "es5"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
